{
  "name": "@deltalaboratory/uuid",
  "version": "0.3.3",
  "description": "A lightweight UUID library for JavaScript/TypeScript with UUIDv4 and UUIDv7 support, offering efficient 22-character encoding and timestamp extraction.",
  "keywords": [
    "uuid"
  ],
  "repository": "https://github.com/DeltaLaboratory/uuid",
  "license": "MIT",
  "author": "delta <delta@deltalab.dev>",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json && esbuild --target=es2015 --minify --bundle src/index.ts --format=cjs --out-extension:.js=.cjs --outdir=dist/cjs",
    "build:esm": "tsc && esbuild --target=es2020 --minify --bundle src/index.ts --format=esm --outdir=dist/esm",
    "format": "prettier --write .",
    "test": "vitest"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.8",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
